{
	"name": "Django Dev Container",
	"dockerComposeFile": "../docker-compose.yaml",
	"service": "web",
	"workspaceFolder": "/app",
	"remoteUser": "root",
	"customizations": {
	  "vscode": {
		"settings": {
		  "python.pythonPath": "/usr/local/bin/python",
		  "python.languageServer": "Pylance"
		},
		"extensions": [
		  "ms-python.python"
		]
	  }
	}
  }
  